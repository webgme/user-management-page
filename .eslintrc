{
  "extends": [
    "eslint:recommended",
    "google",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 6,
    "sourceType": "module"

  },
  "plugins": [
    "react"
  ],
  "rules": {
    "indent": ["error", 4],
    "linebreak-style": 0,
    "max-len": ["error", 120],
    "no-mixed-requires": 1,
    "no-useless-constructor": 0,
    "one-var": 1,
    "padded-blocks": 0,
    "react/prop-types": 0
  }
}